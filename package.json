{
  "name": "covid19mx",
  "description": "Home of historical data about COVID-19 in Mexico updated daily",
  "version": "1.13.3",
  "private": true,
  "engines": {
    "node": "12.x.x",
    "yarn": "1.21.x || 1.22.x"
  },
  "scripts": {
    "build": "gatsby build",
    "check": "yarn check:eslint && yarn check:prettier && yarn check:tsc",
    "check:eslint": "eslint . --ext .jsx,.ts,.tsx",
    "check:prettier": "prettier --check \"src/**/*.{ts,tsx}\"",
    "check:tsc": "yarn tsc",
    "clean": "gatsby clean",
    "commit": "git-cz",
    "data:parse": "node scripts/parse/index.js",
    "data:historical": "node scripts/historical/index.js",
    "develop": "gatsby develop",
    "develop:local": "gatsby develop -H 0.0.0.0",
    "semantic-release": "semantic-release",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "prettier:write": "prettier src/data/historicalTotal.json --write && prettier src/data/historicalNewDaily.json --write"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.28",
    "@fortawesome/free-brands-svg-icons": "5.13.0",
    "@fortawesome/free-solid-svg-icons": "5.13.0",
    "@fortawesome/react-fontawesome": "0.1.9",
    "@svg-maps/mexico": "1.0.0",
    "adm-zip": "0.4.14",
    "amplitude-js": "5.10.0",
    "apexcharts": "3.18.1",
    "axios": "0.19.2",
    "babel-plugin-styled-components": "1.10.6",
    "csv-parse": "4.8.8",
    "csv-stringify": "5.3.6",
    "gatsby": "2.18.12",
    "gatsby-image": "2.2.34",
    "gatsby-plugin-manifest": "2.2.31",
    "gatsby-plugin-offline": "3.0.27",
    "gatsby-plugin-postcss": "2.2.1",
    "gatsby-plugin-react-helmet": "3.1.16",
    "gatsby-plugin-sharp": "2.3.5",
    "gatsby-plugin-styled-components": "3.2.4",
    "gatsby-plugin-typescript": "2.1.26",
    "gatsby-plugin-typescript-checker": "1.1.1",
    "gatsby-source-filesystem": "2.1.40",
    "gatsby-transformer-sharp": "2.3.7",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "moment-timezone": "0.5.28",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-apexcharts": "1.3.7",
    "react-dom": "16.12.0",
    "react-ga": "2.7.0",
    "react-helmet": "5.2.1",
    "react-svg-map": "2.0.2",
    "react-visibility-sensor": "5.1.1",
    "styled-components": "5.1.0",
    "tailwind-percentage-heights-plugin": "0.1.4",
    "tailwindcss": "1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/git": "7.0.18",
    "@types/amplitude-js": "5.8.0",
    "@types/lodash": "4.14.150",
    "@types/moment-timezone": "0.5.13",
    "@types/styled-components": "4.4.2",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "husky": "3.1.0",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "semantic-release": "17.2.3",
    "typescript": "3.7.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn run check:eslint && pretty-quick --verbose --check --staged"
    }
  }
}
